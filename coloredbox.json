    "eula": "",
    "license": "",
    "webcomponents": [
        {
            "kind": "main",
            "tag": "com-sap-sample-coloredbox",
            "url": "https://raw.githubusercontent.com/GabrielGH-dot/custom-widget-table/main/coloredbox.js",
            "integrity": "sha256-2047B2E9B5744CD148ACC93FD3E04EBB1E6575F54F6FC552724E8279A60CDE35",
            "ignoreIntegrity": false
        },
        {
            "kind": "styling",
            "tag": "com-sap-sample-coloredbox-styling",
            "url": "https://raw.githubusercontent.com/GabrielGH-dot/custom-widget-table/main/coloredbox_styling.js",
            "integrity": "sha256-4A72F84F5D5424FFDB6CDADF48760C4F16BBB0AD9CE54E8AE9A9BDA0C6F6B072",
            "ignoreIntegrity": false
        },
        {
            "kind": "builder",
            "tag": "com-sap-sample-coloredbox-builder",
            "url": "https://raw.githubusercontent.com/GabrielGH-dot/custom-widget-table/main/coloredbox_builder.js",
            "integrity": "sha256-CCFF95856C394A9BC9603DD02FA6F422ECCEBC05E87A2C75794038F313A191C3",
            "ignoreIntegrity": false
        }
    ],
    "properties": {
        "color": {
            "type": "string",
            "description": "Background color",
            "default": "red"
        },
        "opacity": {
            "type": "number",
            "description": "Opacity",
            "default": 1
        },
        "width": {
            "type": "integer",
            "default": 100
        },
        "height": {
            "type": "integer",
            "default": 100
        }
    },
    "methods": {
        "setColor": {
            "description": "Sets the background color.",
            "parameters": [
                {
                    "name": "newColor",
                    "type": "string",
                    "description": "The new background color"
                }
            ],
            "body": "this.color = newColor;"
        },
        "getColor": {
            "returnType": "string",
            "description": "Returns the background color.",
            "body": "return this.color;"
        }
    },
    "events": {
        "onClick": {
            "description": "Called when the user clicks the Colored Box."
        }
    }
}